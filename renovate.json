{
  "$schema" : "https://docs.renovatebot.com/renovate-schema.json",
  "assignAutomerge" : true,
  "assigneesFromCodeOwners" : true,
  "reviewersFromCodeOwners" : true,
  "customManagers" : [ {
    "customType" : "regex",
    "description" : "Update _VERSION variables in Dockerfiles",
    "fileMatch" : [ "^images/.*/Dockerfile$" ],
    "matchStrings" : [ "# renovate: datasource=(?<datasource>[a-z-]+?) depName=(?<depName>.+?)(?: (?:packageName|lookupName)=(?<packageName>.+?))?(?: versioning=(?<versioning>[a-z-]+?))?\\s(?:ENV|ARG) .+?_VERSION=(?<currentValue>.+?)\\s" ]
  } ],
  "packageRules" : [ {
    "groupName" : "org.qubership.profiler",
    "matchPackageNames" : [ "org.qubership.profiler{/,}**" ]
  } ],
  "extends" : [ "github>Netcracker/qubership-core-infra:renovate-core-config" ]
}